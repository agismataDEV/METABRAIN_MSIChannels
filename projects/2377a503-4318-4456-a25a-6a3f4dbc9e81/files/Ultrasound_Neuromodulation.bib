@article{krasovitskiIntramembraneCavitationUnifying2011,
  title = {Intramembrane Cavitation as a Unifying Mechanism for Ultrasound-Induced Bioeffects},
  author = {Krasovitski, Boris and Frenkel, Victor and Shoham, Shy and Kimmel, Eitan},
