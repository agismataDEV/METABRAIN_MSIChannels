@article{sorumTensionActivationMechanosensitive2024,
  title = {Tension Activation of Mechanosensitive Two-Pore Domain {{K}}+ Channels {{TRAAK}}, {{TREK-1}}, and {{TREK-2}}},
  author = {Sorum, Ben and Docter, Trevor and Panico, Vincent and Rietmeijer, Robert A. and Brohawn, Stephen G.},
  year = 2024,
