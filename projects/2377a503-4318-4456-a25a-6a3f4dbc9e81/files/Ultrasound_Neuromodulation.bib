@article{prawittTRPM5TransientCa22003,
  title = {{{TRPM5}} Is a Transient {{Ca}}{\textsuperscript{2+}} -Activated Cation Channel Responding to Rapid Changes in [{{Ca}}{\textsuperscript{2+}} ]{\textsubscript{i}}},
  author = {Prawitt, Dirk and {Monteilh-Zoller}, Mahealani K. and Brixel, Lili and Spangenberg, Christian and Zabel, Bernhard and Fleig, Andrea and Penner, Reinhold},
