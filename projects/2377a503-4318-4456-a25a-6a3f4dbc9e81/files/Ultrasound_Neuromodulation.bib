@article{shinPiezo2IonChannel2019,
  title = {The {{Piezo2}} Ion Channel Is Mechanically Activated by Low-Threshold Positive Pressure},
  author = {Shin, Kyung Chul and Park, Hyun Ji and Kim, Jae Gon and Lee, In Hwa and Cho, Hawon and Park, Chanjae and Sung, Tae Sik and Koh, Sang Don and Park, Sang Woong and Bae, Young Min},
